    <script type="text/javascript">
        $(function() {
            if ($("#calendar").length == 0)
                return;
                
            InitCalendar();
        });

        function GetFromDate() {

            var sQueryDate = getParameterByName("From");

            if (sQueryDate != undefined && sQueryDate.length > 0)
                return new Date(sQueryDate);
            else 
                return new Date(new Date().getFullYear(), 0, 1);
        }

        function GetToDate() {

            var sQueryDate = getParameterByName("To");

            if (sQueryDate != undefined && sQueryDate.length > 0)
                return new Date(sQueryDate);
            else
                return new Date(new Date().getFullYear(), 11, 31);                          
        }
        
        function InitCalendar() {

            var events = eval($("#ContentPlaceholder1_hfEventJSON").val());

            var fIncludesWeekends = false;

            for (var i = 0; i < events.length; i++) {
                //safari does not support dashes in the date
                var sDate = events[i].start.replace("-", "/").replace("-", "/").substring(0, events[i].start.indexOf(" "));
                var eventDay = new Date(sDate).getDay(); 
                if (eventDay == 6 || eventDay == 0) 
                    fIncludesWeekends = true;                
            }
            
            var dSelectedDate = new Date();

            //if a different year was chosen, start with the date specified in the FROM
            //otherwise default to the current date
            
            var dFromDate = GetFromDate();
            var dToDate = GetToDate();
                                 
            //if today's date does not fall between the From and To, then default to the From date
            if (dSelectedDate < dFromDate || dSelectedDate > dToDate)
                dSelectedDate = dFromDate;

            var sMonth = getParameterByName("Month");
            if (sMonth != undefined && sMonth.length > 0)
                dSelectedDate.setMonth(parseInt(sMonth) - 1, 1);
                 
			$('#calendar').fullCalendar(
            {
                header:
                {
                    left: 'title',
                    center: '',
                    right: 'prev,next today'
                },
                editable: false,
                weekends: fIncludesWeekends,
                events: events,
                year: dSelectedDate.getFullYear(),
                month: dSelectedDate.getMonth(),
                eventRender: RenderEvent,
                viewDisplay: UpdateCalendarNavButtons,
                weekMode: "variable"
            });

            if(typeof FixFooterPosition == 'function')
                FixFooterPosition();

            // Trying to fix printing 
            $(".fc-widget-header").each(function () {
                $(this).css("width","14.28%");
            });
        }

        function UpdateCalendarNavButtons(view) {

            var dDisplayedDate = view.calendar.getDate();

            var dFromDate = GetFromDate();
            var dToDate = GetToDate();
            var dToday = new Date();

            var fIsBeginning = (dDisplayedDate.getMonth() == dFromDate.getMonth() && dDisplayedDate.getFullYear() == dFromDate.getFullYear());
            var fIsEnd = (dDisplayedDate.getMonth() == dToDate.getMonth() && dDisplayedDate.getFullYear() == dToDate.getFullYear());
            var fIsCurrentYear = (dToday >= dFromDate && dToday <= dToDate);

            if (fIsBeginning)
                $(".fc-button-prev").click(GotoPrevYear);
            else
                $(".fc-button-prev").unbind("click", GotoPrevYear);
                
            if (fIsEnd)
                $(".fc-button-next").click(GotoNextYear);
            else
                $(".fc-button-next").unbind("click", GotoNextYear);


            if (fIsCurrentYear)
                $(".fc-button-today").css("display", "");
            else
                $(".fc-button-today").css("display", "none");

        }

        function GotoNextYear() { GotoNextPrevYear(true); }
        function GotoPrevYear() { GotoNextPrevYear(false); }
        
        function GotoNextPrevYear(fNext) 
        {
            
            if(fNext)
            {
                var dFrom = new Date(GetFromDate().getFullYear()+1, GetFromDate().getMonth(), GetFromDate().getDate());
                var dTo = new Date(GetToDate().getFullYear() + 1, GetToDate().getMonth(), GetToDate().getDate());
                var iStartMonth = 1;
             }
             else
             {
                 var dFrom = new Date(GetFromDate().getFullYear() - 1, GetFromDate().getMonth(), GetFromDate().getDate());
                 var dTo = new Date(GetToDate().getFullYear() - 1, GetToDate().getMonth(), GetToDate().getDate());
                 var iStartMonth = 12;
             }
             
             var sFrom = (dFrom.getMonth()+1) + "/" + dFrom.getDate() + "/" + dFrom.getFullYear();
             var sTo = (dTo.getMonth() + 1) + "/" + dTo.getDate() + "/" + dTo.getFullYear();

             $('#calendar').html("<img src='/Lib/images/Loading_Snake_32x32.gif' alt='Please wait...'/>");
             window.location = "Calendar.aspx?View=Calendar&From=" + sFrom + "&To=" + sTo + "&Month=" + iStartMonth;

        }

        var miNextID = 1

        //thought we had this function somewhere else but i can't find it
        //http://stackoverflow.com/questions/901115/get-query-string-values-in-javascript
        function getParameterByName(name) {
            name = name.replace(/[\[]/, "\\\[").replace(/[\]]/, "\\\]");
            var regexS = "[\\?&]" + name + "=([^&#]*)";
            var regex = new RegExp(regexS);
            var results = regex.exec(window.location.search);
            if (results == null)
                return "";
            else
                return decodeURIComponent(results[1].replace(/\+/g, " "));
        } 
        
        function RenderEvent(event, element) 
        {
            var iHour = event.start.getHours();
            var sAMorPM = 'am';
            if (iHour >= 12) sAMorPM = 'pm';

            if (iHour > 12) 
            {
                iHour -= 12;
            }
            else if (iHour == 0) 
            {
                iHour = 12;
            }

            var sTime = iHour + ":" + pad(event.start.getMinutes(), 2) + sAMorPM;

            var sDownloads = DecodeFromServer(unescape(event.downloads));
            var sDownloadsBtn = "";

            if (sDownloads.length > 0) 
                sDownloadsBtn = "<br/><a id='lnkShowDownloads" + miNextID + "' href='javascript:ShowDownloads(" + miNextID + ");'>Show Downloads</a><div class='CalendarDownloads' id='divDownloads" + miNextID + "' style='display:none'>" + sDownloads + "</div>";                

            miNextID++;

            //bit of a hack, title looks better with each part on a new line
            var sTitle = DecodeFromServer(event.title).replace(" - ", "<br/>");

            var sVideo = "";

            if (event.video != undefined)
                sVideo = "<br/>" + DecodeFromServer(unescape(event.video));

            var sCancelled = "";

            if (event.cancelled == 'True')
                sCancelled = '<div class="MeetingCancelled">Cancelled</div>';
                
            return "<div class='MeetingCalendarCell MainScreenText'>" + sCancelled + "<a href='" + event.url + "'>" + sTime + "</a><br/>" + sTitle + sVideo + sDownloadsBtn + "</div>"
        }

        function ShowDownloads(id) {

            //in case there are other boxes in the same cell, we need to make sure their z-index is lowered so the new box
            //will always be in front
            for (var i = 0; i < miNextID; i++)
                $("#divDownloads" + i).parent().css("z-index", 999);
                        
            if ($("#divDownloads" + id).css("display") == "none") {
                $("#lnkShowDownloads" + id).html("Hide Downloads");
                $("#divDownloads" + id).slideDown("fast");
                $("#divDownloads" + id).parent().css("z-index", 1000);
            }
            else {
                $("#lnkShowDownloads" + id).html("Show Downloads");
                $("#divDownloads" + id).slideUp("fast");
            }
        }

        function pad(num, size) {
            var s = num + "";
            while (s.length < size) s = "0" + s;
            return s;
        } 


    </script>
    
